<?xml version="1.0" encoding="utf-8"?>
<engine:UXML
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:engine="UnityEngine.UIElements"
    xmlns:mmui="MasterMemory.Sample.UI"
    xsi:noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd"
>
    <engine:Style src="/Assets/Styles/generated.uss"/>
    <engine:Label text="Commands" class="text-xl"/>
    <engine:IntegerField label="A">
        <Bindings>
            <engine:DataBinding property="value" data-source-path="a"/>
        </Bindings>
    </engine:IntegerField>

    <engine:IntegerField label="B">
        <Bindings>
            <engine:DataBinding property="value" data-source-path="b"/>
        </Bindings>
    </engine:IntegerField>

    <engine:Label>
        <Bindings>
            <engine:DataBinding property="text" data-source-path="Result" binding-mode="ToTarget"/>
        </Bindings>
    </engine:Label>

    <engine:Label text="status">
        <Bindings>
            <engine:DataBinding property="text" data-source-path="status" binding-mode="ToTarget"/>
        </Bindings>
    </engine:Label>

    <mmui:DispatchButton event="add" text="Run" class="h-10"/>
</engine:UXML>
